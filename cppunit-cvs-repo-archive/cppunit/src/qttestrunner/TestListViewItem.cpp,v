#include "TestListViewItem.h"


TestListViewItem::TestListViewItem( CPPUNIT_NS::Test *test,
                                    QListViewItem *parent ) : 
    QListViewItem( parent ),
    _test( test )
{
}


TestListViewItem::~TestListViewItem()
{
}


CPPUNIT_NS::Test *
TestListViewItem::test() const
{
  return _test;
}
